<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="172.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.StopWatchController">
   <children>
      <Button fx:id="startButton" layoutX="51.0" layoutY="114.0" mnemonicParsing="false" onAction="#StartButtonPressed" text="Start" />
      <Button fx:id="stopButton" layoutX="122.0" layoutY="114.0" mnemonicParsing="false" onAction="#StopButtonPressed" text="Stop" />
      <Button fx:id="resetButton" layoutX="190.0" layoutY="114.0" mnemonicParsing="false" onAction="#ResetButtonPressed" text="Reset" />
      <TextField fx:id="timer" layoutX="62.0" layoutY="59.0" text="0s" />
   </children>
</AnchorPane>
